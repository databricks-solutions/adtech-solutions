    {
      "name": "7c7fcaff",
      "displayName": "Usage by workspace ID",
      "queryLines": [
        "WITH user_usage AS (\n",
        "SELECT\n",
        "  identity_metadata.run_as as user_email, \n",
        "  usage_quantity\n",
        "FROM system.billing.usage\n",
        "WHERE usage_date >= CURRENT_DATE() - INTERVAL 90 DAYS\n",
        " AND identity_metadata.run_as is not null\n",
        " and identity_metadata.run_as like '%@%'\n",
        ")\n",
        "SELECT\n",
        "  user_email,\n",
        "  SUM(usage_quantity) AS total_dbus\n",
        "FROM user_usage\n",
        "GROUP BY user_email\n",
        "ORDER BY total_dbus DESC\n",
        "LIMIT 10;"
      ]
    },
    {
      "name": "813121ab",
      "displayName": "audit",
      "queryLines": [
        "SELECT\n",
        "  user_identity.email as user_email,\n",
        "  COUNT(*) as webterminal_count\n",
        "FROM system.access.audit \n",
        "WHERE service_name = 'webTerminal' \n",
        "  AND action_name = 'startSession'\n",
        "  AND event_date > now() - interval 90 day\n",
        "GROUP BY user_identity.email\n",
        "ORDER BY webterminal_count DESC\n",
        "LIMIT 10\n"
      ]
    }
  ],
