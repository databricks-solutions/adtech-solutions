{
  "datasets": [
    {
      "name": "15f3fa49",
      "displayName": "Total Individuals",
      "queryLines": [
        "select count(distinct megacorp_indid) as num_inds\n",
        "from achu_demos.segments.megacorp_campaigns\n",
        "where campaign_name = :Campaign\n",
        "limit 10\n",
        "\n",
        "-- achu_demos_data_collab.profiles.partner1_audience_census_profile"
      ],
      "parameters": [
        {
          "displayName": "Campaign",
          "keyword": "Campaign",
          "dataType": "STRING",
          "defaultSelection": {
            "values": {
              "dataType": "STRING",
              "values": [
                {
                  "value": "Happy Dogs"
                }
              ]
            }
          }
        }
      ]
    },
    {
      "name": "0d627cae",
      "displayName": "Campaign Age Range",
      "queryLines": [
        "with current_campaign as (\n",
        "select *\n",
        "from achu_demos.segments.megacorp_campaigns\n",
        "--where campaign_name =  :Campaign\n",
        ")\n",
        "select coalesce(CASE\n",
        "           WHEN age >= 18 AND age <= 24 THEN '18-24'\n",
        "           WHEN age >= 25 AND age <= 34 THEN '25-34'\n",
        "           WHEN age >= 35 AND age <= 44 THEN '35-44'\n",
        "           WHEN age >= 45 AND age <= 55 THEN '45-55'\n",
        "           WHEN age > 55 THEN '55+'\n",
        "       END , 'Unknown') as age_range, count(distinct a.megacorp_indid) as num_individuals, campaign_name AS Segment\n",
        "from current_campaign a join achu_demos.profiles.megacorp_audience_census_profile b on a.megacorp_indid = b.megacorp_indid\n",
        "group by campaign_name, age_range\n",
        "order by age_range\n",
        "-- limit 10"
      ]
    },
    {
      "name": "e13a8338",
      "displayName": "Campaign Gender Breakdown",
      "queryLines": [
        "with current_campaign as (\n",
        "select *\n",
        "from achu_demos.segments.megacorp_campaigns\n",
        "--where campaign_name =  :Campaign\n",
        ")\n",
        "select coalesce(gender, 'Unknown') as gender, count(distinct a.megacorp_indid) as num_individuals, campaign_name AS Segment\n",
        "from current_campaign a join achu_demos.profiles.megacorp_audience_census_profile b on a.megacorp_indid = b.megacorp_indid\n",
        "group by campaign_name, gender\n",
        "order by gender\n",
        "-- limit 10"
      ]
    },
    {
      "name": "d9335995",
      "displayName": "Match Funnel",
      "queryLines": [
        "with campaign_inds as (\n",
        "  select distinct megacorp_indid\n",
        "  from achu_demos.segments.megacorp_campaigns\n",
        "  where campaign_name = :Campaign\n",
        "),\n",
        "email_indids as (\n",
        "  select distinct email_sha256\n",
        "  from achu_demos.crosswalks.megacorp_audience_digital_graph a\n",
        "  join campaign_inds b\n",
        "  on a.megacorp_indid = b.megacorp_indid\n",
        "),\n",
        "match_rate as (\n",
        "  select market_share\n",
        "  from achu_demos.bronze.synthetic_distributor_metrics\n",
        "  where distributor_name = :Distributor\n",
        "),\n",
        "num_ids as (\n",
        "  select count(*) as n\n",
        "  from campaign_inds\n",
        "),\n",
        "num_emails as (\n",
        "  select count(*) as n\n",
        "  from email_indids\n",
        "),\n",
        "total_mc_ids as (\n",
        "  select count(distinct megacorp_indid) as n\n",
        "  from achu_demos.crosswalks.megacorp_audience_digital_graph\n",
        "),\n",
        "total_mc_emails as (\n",
        "  select count(distinct email_sha256) as n\n",
        "  from achu_demos.crosswalks.megacorp_audience_digital_graph\n",
        ")\n",
        "\n",
        "SELECT 'Individuals in Segment' AS match_funnel_step, n AS num_ids, ROUND(n/(select n from total_mc_ids),4) as pct_universe\n",
        "FROM num_ids\n",
        "UNION ALL\n",
        "SELECT 'Matched Emails from MegaCorp Universe', n AS num_ids, ROUND(n/(select n from total_mc_emails),4)\n",
        "FROM num_emails\n",
        "UNION ALL\n",
        "SELECT 'Matched Emails to Partner Universe', ROUND(n * (select market_share from match_rate)) AS num_ids, ROUND(n/(select n from total_mc_emails),4) AS pct_universe\n",
        "FROM num_emails"
      ],
      "parameters": [
        {
          "displayName": "Campaign",
          "keyword": "Campaign",
          "dataType": "STRING",
          "defaultSelection": {
            "values": {
              "dataType": "STRING",
              "values": [
                {
                  "value": "Happy Dogs"
                }
              ]
            }
          }
        },
        {
          "displayName": "Distributor",
          "keyword": "Distributor",
          "dataType": "STRING",
          "defaultSelection": {
            "values": {
              "dataType": "STRING",
              "values": [
                {
                  "value": "Rainbow Hemisphere"
                }
              ]
            }
          }
        }
      ]
    },
